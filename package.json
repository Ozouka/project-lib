{
  "name": "shopify-test-karim-iwci",
  "description": "Customizable modular development environment for blazing-fast Shopify theme creation",
  "author": "IWCI - Alexis Garcia",
  "version": "1.0.0",
  "license": "MIT",
  "packageManager": "pnpm@7.12.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently --raw --kill-others --passthrough-arguments \"pnpm run shopify:dev --store=test-karim-iwci.myshopify.com\" \"pnpm run vite:dev\" --",
    "build": "vite build",
    "deploy": "pnpm run build && pnpm run shopify:deploy",
    "format": "prettier -w . --cache --plugin-search-dir=.",
    "lint": "eslint .",
    "pull": "shopify theme pull",
    "pull:dev": "shopify theme pull --development",
    "shopify:dev": "shopify theme dev --live-reload full-page",
    "shopify:deploy": "shopify theme push",
    "vite:dev": "vite"
  },
  "devDependencies": {
    "@shopify/eslint-plugin": "^41.3.1",
    "@shopify/prettier-plugin-liquid": "^1.4.0",
    "@types/alpinejs": "^3.13.5",
    "@types/postcss-import": "^14.0.3",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.55.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.32",
    "postcss-import": "^15.1.0",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "tailwindcss": "^3.3.6",
    "typescript": "^4.9.5",
    "vite": "^4.5.1",
    "vite-plugin-checker": "^0.6.2",
    "vite-plugin-shopify": "^2.2.1"
  },
  "dependencies": {
    "@shopify/cli": "^3.52.0",
    "@shopify/theme": "^3.52.0",
    "alpinejs": "^3.13.3",
    "embla-carousel": "^8.5.2",
    "embla-carousel-autoplay": "^8.5.2",
    "embla-carousel-class-names": "^8.5.2"
  },
  "engines": {
    "node": ">= 16.14.2"
  }
}
